{
  "@context": {
    "@version": 1.1,
    "id": "@id",
    "type": "@type",
    "schema": "https://www.schema.org/",
    "cred": "https://gitlab.hs-anhalt.de/stmosarw/projekt-anwendungsentwicklung/schemas/credentials/student.jsonld#",
    "StudentCredential": {
      "@id": "cred:StudentCredential",
      "@context": {
        "@version": 1.1,
        "id": "@id",
        "type": "@type",
        "StudySubject": {
          "@id": "cred:StudySubject",
          "@context": {
            "@version": 1.1,
            "id": "@id",
            "type": "@type",
            "provider": {
              "@id": "schema:legalName",
              "@type": "schema:Text"
            },
            "name": {
              "@id": "schema:Name",
              "@type": "schema:Text"
            },
            "degree": {
              "@id": "schema:educationalCredentialAwarded",
              "@type": "schema:Text"
            },
            "credits": {
              "@id": "schema:numberOfCredits",
              "@type": "schema:Integer"
            },
            "termsPerYear": {
              "@id": "schema:termsPerYear",
              "@type": "cred:Student"
            }
          }
        },
        "matriculationNumber": {
          "@id": "cred:matriculationNumber",
          "@type": "schema:Integer"
        },
        "currentTerm": {
          "@id": "cred:currentTerm",
          "@type": "schema:Integer"
        },
        "Student": {
          "@id": "cred:Student",
          "@context": {
            "@version": 1.1,
            "id": "@id",
            "type": "@type",
            "firstName": {
              "@id": "schema:firstName",
              "@type": "schema:Text"
            },
            "middleNames": {
              "@id": "schema:additionalNames",
              "@type": "schema:Text"
            },
            "familyName": "schema:familyName",
            "birthDate": "schema:Text"
          }
        },
        "photoURL": {
          "@id": "schema:url",
          "@type": "@id"
        }
      }
    }
  }
}