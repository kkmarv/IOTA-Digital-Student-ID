{
  "@context": {
    "@version": 1.1,
    "id": "@id",
    "type": "@type",
    "schema": "https://www.schema.org/",
    "cred": "https://gitlab.hs-anhalt.de/stmosarw/projekt-anwendungsentwicklung/schemas/credentials/student.jsonld#",
    "StudentCredential": {
      "@id": "cred:StudentCredential",
      "@context": {
        "@version": 1.1,
        "id": "@id",
        "type": "@type",
        "courseOfStudy": {
          "@id": "cred:courseOfStudy",
          "@type": "schema:EducationalOccupationalProgram",
          "@context": {
            "@version": 1.1,
            "id": "@id",
            "type": "@type",
            "degree": {
              "@id": "schema:educationalCredentialAwarded",
              "@type": "schema:Text"
            },
            "name": {
              "@id": "cred:name",
              "@type": "schema:Name"
            },
            "credits": {
              "@id": "schema:numberOfCredits",
              "@type": "schema:Integer"
            },
            "termsPerYear": {
              "@id": "cred:termsPerYear",
              "@type": "schema:Integer"
            }
          }
        },
        "institutionName": {
          "@id": "schema:legalName",
          "@type": "schema:Text"
        },
        "matriculationNumber": {
          "@id": "cred:matriculationNumber",
          "@type": "schema:Integer"
        },
        "currentTerm": {
          "@id": "cred:currentTerm",
          "@type": "schema:Integer"
        },
        "photo": {
          "@id": "schema:Photograph",
          "@type": "@id"
        }
      }
    }
  }
}